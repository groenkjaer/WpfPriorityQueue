<Window x:Class="WpfPriorityQueue.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPriorityQueue"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Label Content="Call Centers Smarte App" Grid.ColumnSpan="3" FontSize="72" Margin="0,30,0,0"/>
        <TextBlock x:Name="txtOpkald" Text="" TextWrapping="Wrap" Grid.Column="2" Grid.Row="1" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Label x:Name="lblTimer" Grid.Column="2" Grid.Row="1" FontSize="30" Margin="0,0,0,0" VerticalAlignment="Bottom" HorizontalAlignment="Center"/>
        <Button x:Name="btnTagOpkald" Content="Tag Opkald" Width="80" Height="30.5" Grid.Row="1" Click="btnTagOpkald_Click" IsDefault="True"/>
        <Button x:Name="btnHangUp" Content="Læg på" Width="80" Height="30.5" Grid.Row="1" Grid.Column="0" Click="btnHangUp_Click" IsEnabled="False" Margin="0,90,0,0"/>
        <ListBox x:Name="lstboxQueue" Grid.Row="1" Grid.Column="1" Margin="0,0,0,10">
            
        
        </ListBox>
    </Grid>
</Window>
